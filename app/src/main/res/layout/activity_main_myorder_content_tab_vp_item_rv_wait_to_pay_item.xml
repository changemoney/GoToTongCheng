<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_activity_main_myorder_content_black_bg"
    android:id="@+id/lly_main_myorder_content_total"
    >
    <gototongcheng.zhyan.com.library.Widget.SwipeLayout.SwipeLayout
        android:id="@+id/sly_main_myorder_content_rv_wait_to_pay_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/activity_main_myorder_tabbar_vp_item_lv_item_height"
        android:background="@drawable/activity_main_myorder_round_shape"
        android:orientation="vertical"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/zero"
            android:orientation="horizontal"
            android:layout_weight="1"
            >
            <ImageView
                android:layout_width="8dp"
                android:layout_height="8dp"
                android:background="@drawable/point_gray"
                android:layout_marginLeft="15dp"
                android:layout_gravity="center_vertical"
                android:layout_marginRight="10dp"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="10sp"
                android:text="@string/orderNum"
                android:textColor="@color/color_activity_main_myorder_tabbar_vpitem_lvitem_gray_word_bg"
                android:layout_gravity="center_vertical"
                android:layout_marginRight="10dp"
                />

            <TextView
                android:id="@+id/tv_myorder_content_tab_vp_wait_to_pay_item_rv_item_orderno"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="10sp"
                android:text="121232445465"
                android:textColor="@color/color_activity_main_myorder_tabbar_vpitem_lvitem_gray_word_bg"
                android:layout_gravity="center_vertical"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/zero"
            android:orientation="vertical"
            android:layout_weight="2.6"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/zero"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                >
                <ImageView
                    android:layout_width="18dp"
                    android:layout_height="20dp"
                    android:background="@drawable/activity_main_myorder_icon_st"
                    android:layout_gravity="center_vertical"
                    />
                <TextView
                    android:id="@+id/tv_myorder_content_tab_vp_wait_to_pay_item_rv_item_beginaddr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/black"
                    android:textSize="@dimen/activity_main_myorder_rv_item_addr_word_size"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:maxLines="2"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/zero"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                >
                <ImageView
                    android:layout_width="18dp"
                    android:layout_height="20dp"
                    android:background="@drawable/activity_main_myorder_icon_en"
                    android:layout_gravity="center_vertical"
                    />
                <TextView
                    android:id="@+id/tv_myorder_content_tab_vp_wait_to_pay_item_rv_item_endaddr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="@color/black"
                    android:textSize="@dimen/activity_main_myorder_rv_item_addr_word_size"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:maxLines="2"
                    />
            </LinearLayout>

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/zero"
            android:layout_weight="1.5"
            >



            <TextView
                android:id="@+id/tv_myorder_content_tab_vp_wait_to_pay_item_rv_item_ordermoney"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/moneyTest"
                android:textColor="@color/color_activity_main_myorder_tabbar_vpitem_lvitem_black_word_bg"
                android:textSize="11sp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"

                />

            <TextView
                android:id="@+id/tv_main_myorder_ordertime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/orderTime"
                android:textColor="@color/color_activity_main_myorder_tabbar_vpitem_lvitem_gray_word_bg"
                android:textSize="10sp"
                android:layout_marginLeft="18dp"
                android:layout_alignBaseline="@+id/tv_myorder_content_tab_vp_item_rv_item_ordermoney"
                android:layout_alignBottom="@+id/tv_myorder_content_tab_vp_item_rv_item_ordermoney"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="18dp" />
            <TextView
                android:id="@+id/tv_myorder_content_tab_vp_wait_to_pay_item_rv_item_ordertime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/orderTimeTest"
                android:textColor="@color/color_activity_main_myorder_tabbar_vpitem_lvitem_black_word_bg"
                android:textSize="10sp"
                android:layout_toRightOf="@id/tv_main_myorder_ordertime"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                />
        </RelativeLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/zero"
            android:layout_weight="1.5"
            android:orientation="horizontal"
            >
            <RelativeLayout
                android:layout_width="@dimen/zero"
                android:layout_height="match_parent"
                android:layout_weight="1"
                >


            </RelativeLayout>
            <RelativeLayout
                android:layout_width="@dimen/zero"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                >
                <Button
                    android:id="@+id/bt_myorder_content_tab_vp_wait_to_pay_item_rv_item_submit"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/activity_main_myorder_wait_to_pay_gray_bg_half_round_radius"
                    android:layout_margin="@dimen/activity_main_myorder_wait_to_pay_submit_margin_size"
                    android:text="@string/toPay"
                    android:textColor="@color/color_activity_main_myorder_content_black_bg"
                    android:textSize="@dimen/activity_main_myorder_wait_to_pay_submit_word_size"
                    />

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="@dimen/zero"
                android:layout_height="match_parent"
                android:layout_weight="1"
                >


            </RelativeLayout>

        </LinearLayout>

    </LinearLayout>
            <RelativeLayout
                android:id="@+id/rly_main_myorder_content_rv_wait_to_pay_item_delete"
                android:layout_width="50dp"
                android:layout_height="@dimen/activity_main_myorder_tabbar_vp_item_lv_item_height"
                android:background="@drawable/activity_main_myorder_red_delete_bg_radius"

                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/delete"
                    android:textColor="@color/color_activity_main_addressmanage_content_white_bg"
                    android:textSize="15sp"
                    android:layout_centerInParent="true"
                    />
            </RelativeLayout>


    </gototongcheng.zhyan.com.library.Widget.SwipeLayout.SwipeLayout>

